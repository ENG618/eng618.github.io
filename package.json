{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/styles": "^4.10.0",
    "gatsby": "^2.22.11",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-material-ui": "^2.1.8",
    "gatsby-plugin-offline": "^3.2.7",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-theme-notes": "^1.3.53",
    "gatsby-transformer-sharp": "^2.5.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "postbuild": "rsync -av public/ ../docs/",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "dev:clean": "gatsby clean && gatsby develop",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.{scss,css,js,md,mdx}\"",
    "format:diff": "prettier --check \"src/**/*.{scss,css,js,md,mdx}\"",
    "lint": "eslint .",
    "serve": "gatsby serve",
    "start": "yarn dev",
    "test": "yarn format:diff"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "endOfLine": "lf"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
