{
  "name": "garciaericn",
  "private": true,
  "description": "A Personal portfolio",
  "version": "0.1.0",
  "author": "Eric N. Garcia <engarcia618@gmail.com>",
  "dependencies": {
    "@narative/gatsby-theme-novela": "^0.12.2",
    "gatsby": "^2.18.18",
    "gatsby-plugin-manifest": "^2.2.34",
    "gatsby-plugin-offline": "^3.0.30",
    "gatsby-theme-notes": "^1.0.41",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "husky": "^4.0.9",
    "prettier": "prettier/prettier",
    "pretty-quick": "^2.0.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "dev:clean": "gatsby clean && gatsby develop",
    "format": "prettier --write \"src/**/*.{scss,css,js,md,mdx}\"",
    "format:diff": "prettier --check \"src/**/*.{scss,css,js,md,mdx}\"",
    "lint": "eslint .",
    "serve": "gatsby serve",
    "start": "yarn dev",
    "test": "yarn format:diff"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "endOfLine": "lf"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
